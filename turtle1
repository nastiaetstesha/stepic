import turtle as t
def square(side):
  for i in range(4):
    
    t.forward(side)
    t.left(90)
    t.forward(side)
    t.left(90)
    t.forward(side)
    t.left(90)
    t.forward(side)
  t.setheading(45)  
  for i in range(4):
    
    t.forward(side)
    t.left(90)
    t.forward(side)
    t.left(90)
    t.forward(side)
    t.left(90)
    t.forward(side)
  
  
  
square(90)
